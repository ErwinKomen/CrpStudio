  <div class="large-14 medium-14 small-14 large-offset-1 medium-offset-1 small-offset-1">
    <div id="result_fetching" class="large-16 medium-16 small-16">
      <div><h4>$labels.getString("result.status"):</h4></div>
      <div id="result_status" class="large-16 medium-16 small-16">
        status
      </div>
      <div id="result_progress" class="large-16 medium-16 small-16 row">
        <!-- Progress indication -->
        <div class="small-8 medium-8 large-8 columns">
          <!-- Blue progress bar indicating which files are started -->
          <div class="progress round"><span id="agent007" class="meter" style="width: 0%"></span></div>
          <!-- Green progress bar indicating which files are finished -->
          <div class="progress success round"><span id="agent008" class="meter" style="width: 0%"></span></div>
        </div>
        <!-- Stop execution -->
        <div id="project_execution" class="large-4 medium-4 small-4 columns">
            <button onclick="Crpstudio.project.reset()" class="reset small secondary">
              <span>$labels.getString("project.cancel")</span>
            </button>
        </div>        
      </div>
      <div id="result_report" class="large-16 medium-16 small-16">

      </div>
    </div>
    <div id="result_querylines" class="large-16 medium-16 small-16 hidden">
      <div><h4>$labels.getString("result.queries"):</h4></div>
      <div class="large-16 medium-16 small-16 row">
        <table id="queries" class="select">
          <thead>
            <tr class="tbl_head">
              <th class="r">#</th>
              <th class="q">$labels.getString("result.qcname")</th>
              <th class="s">$labels.getString("result.subcat")</th>
              <th class="c">$labels.getString("result.count")</th>
              <th class="control"></th>
            </tr>
          </thead>
          <tbody>

          </tbody>
        </table>
      </div>
    </div>
    <div id="results" class="result-content">
      <div><h4>$labels.getString("result.results"):</h4></div>
      <div class="large-16 medium-16 small-16">
        <!-- tabs -->
        <ul class="nav nav-tabs" id="contentTabs">
          #foreach ($tabspec in $tabspecs)
            #set ($view = $tabspec.getView())
            #set ($tabLabel = "result.tab" + $tabspec.getView())
            #set ($tabName = $labels.getString($tabLabel))
            #if($view == "1")
              <li class="active disabled"><a onclick="Crpstudio.project.update($view)">$tabName</a></li>
            #else
              <li><a onclick="Crpstudio.project.update($view)">$tabName</a></li>
            #end
          #end
          <!-- 
          <li class="active disabled"><a onclick="Crpstudio.project.update(1)">$labels.getString("result.tab1")</a></li>
          <li><a onclick="Crpstudio.project.update(2)">$labels.getString("result.tab2")</a></li>
          <li><a onclick="Crpstudio.project.update(3)">$labels.getString("result.tab3")</a></li>
          <li><a onclick="Crpstudio.project.update(4)">$labels.getString("result.tab4")</a></li>
          -->
        </ul>
        <!-- there are a number of tabs, specified by "result.tab.names" -->
        #foreach ($tabspec in $tabspecs)
          #set ($view = $tabspec.getView())
          #set ($tabId = $tabspec.getName())
          #set ($resultsTimeId = "results_time_" + $view)
          #set ($resultsExportId = "results_export_" + $view)
          #set ($resultsFileId = "results_export_file_" + $view)
          #set ($resultsTableId = "result_table_" + $view)
          #set ($resultStatus = "result_status_" + $view)
          #if($view == "1")
            <div id="$tabId" class="result-pane tab-pane active lightbg haspadding">
          #else
            <div id="$tabId" class="result-pane tab-pane lightbg haspadding">
          #end
            <input type="hidden" class="current-view" value="$view" />
            <div class="gradient"></div>
            <div id="$resultStatus" class="pagination large-16 medium-16 small-16 row"></div>
            <!-- export button -->
            <div class="export large-16 medium-16 small-16 row">
              <div class="large-3 medium-3 small-4 columns">
                <button class="small" onclick="Crpstudio.result.doExport($view);">$labels.getString("result.export")</button>
              </div>
              <div id="$resultsTimeId" class="large-4 medium=4 small-4 columns"></div>
              <div id="$resultsExportId" class="large-9 medium=9 small-8 columns hidden">
                <span>$labels.getString("result.export.intro"): </span>
                <span id="$resultsFileId"></span>
              </div>
            </div>
            <!-- result table for this view -->
            <div id="$resultsTableId" class="large-16 medium-16 small-16">
                  <i>(Working...)</i>
            </div>
          </div>
        #end
      </div>
    </div>
  </div>